{
  "name": "cloneui",
  "version": "0.0.6",
  "description": "A modern component React",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "build": "father build",
    "build:watch": "father dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "prepare": "husky install && dumi setup",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{test,utils,types,components}/**/*.{css,less}\"",
    "lint:es": "eslint \"{test,utils,types, components}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build",
    "changelog": "auto-changelog -p"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "sideEffects": false,
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": true,
    "commitLimit": false
  },
  "files": [
    "es",
    "lib",
    "dist",
    "package.json",
    "README.md"
  ],
  "authors": [
    "Yudi <myudisobari12@gmail.com>"
  ],
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "@ant-design/cssinjs-utils": "^1.1.3"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js"
    }
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@rc-component/color-picker": "~2.0.1",
    "@rc-component/mutate-observer": "^1.1.0",
    "@rc-component/qrcode": "~1.0.0",
    "@rc-component/tour": "~1.15.1",
    "@rc-component/trigger": "^2.3.0",
    "chalk": "^5.5.0",
    "chroma-js": "^3.1.2",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "polished": "^4.3.1",
    "rc-cascader": "~3.34.0",
    "rc-checkbox": "~3.5.0",
    "rc-collapse": "~3.9.0",
    "rc-dialog": "^10.0.0",
    "rc-drawer": "~7.3.0",
    "rc-dropdown": "~4.2.1",
    "rc-field-form": "~2.7.0",
    "rc-footer": "^0.6.8",
    "rc-image": "~7.12.0",
    "rc-input": "~1.8.0",
    "rc-input-number": "~9.5.0",
    "rc-mentions": "~2.20.0",
    "rc-menu": "~9.16.1",
    "rc-motion": "^2.9.5",
    "rc-notification": "~5.6.4",
    "rc-pagination": "~5.1.0",
    "rc-picker": "~4.11.3",
    "rc-progress": "~4.0.0",
    "rc-rate": "~2.13.1",
    "rc-resize-observer": "^1.4.3",
    "rc-segmented": "~2.7.0",
    "rc-select": "~14.16.8",
    "rc-slider": "~11.1.8",
    "rc-steps": "~6.0.1",
    "rc-switch": "~4.1.0",
    "rc-table": "~7.51.1",
    "rc-tabs": "~15.7.0",
    "rc-textarea": "~1.10.2",
    "rc-tooltip": "~6.4.0",
    "rc-tree": "~5.13.1",
    "rc-tree-select": "~5.27.0",
    "rc-upload": "~4.9.2",
    "rc-util": "^5.44.4",
    "throttle-debounce": "^5.0.2",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@ant-design/cssinjs-utils": "^1.1.3",
    "@antfu/eslint-config": "^5.2.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.1",
    "@types/chroma-js": "^3.1.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/throttle-debounce": "^5.0.2",
    "@umijs/lint": "^4.0.0",
    "auto-changelog": "^2.5.0",
    "babel-plugin-import": "^1.13.8",
    "cross-env": "^10.0.0",
    "dumi": "^2.4.13",
    "dumi-plugin-color-chunk": "^2.1.0",
    "dumi-theme-antd-style": "^0.31.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scan": "^0.4.3",
    "stylelint": "^14.9.1",
    "tsx": "^4.20.3"
  }
}
